<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.GameSettingsController">
   <children>
      <AnchorPane layoutX="200.0" layoutY="100.0" prefHeight="200.0" prefWidth="200.0" style="-fx-border-color: black;">
         <children>
            <Button fx:id="cancelButton" layoutX="14.0" layoutY="161.0" mnemonicParsing="false" onAction="#cancelButtonOnAction" text="Cancel" />
            <Button fx:id="acceptButton" layoutX="134.0" layoutY="161.0" mnemonicParsing="false" onAction="#acceptButtonOnAction" text="Accept" />
            <Label fx:id="rowsLabel" layoutX="17.0" layoutY="33.0" text="Rows" />
            <Label fx:id="columnsLabel" layoutX="17.0" layoutY="67.0" text="Columns" />
            <Label fx:id="dificultylevelLabel" layoutX="17.0" layoutY="90.0" prefHeight="37.0" prefWidth="52.0" text="Dificulty Level" wrapText="true" />
            <MenuButton fx:id="levelMenuButton" layoutX="66.0" layoutY="96.0" mnemonicParsing="false" onAction="#levelManuButtonOnAction" prefHeight="26.0" prefWidth="98.0" text="Level">
              <items>
                <MenuItem fx:id="level1MenuItem" mnemonicParsing="false" onAction="#level1MenuItemOnAction" text="Level 1" />
                <MenuItem fx:id="level2MenuItem" mnemonicParsing="false" onAction="#level2MenuItemOnAction" text="Level 2" />
                  <MenuItem fx:id="level3MenuItem" mnemonicParsing="false" onAction="#level3MenuItemOnAction" text="Level 3" />
              </items>
            </MenuButton>
            <TextField fx:id="rowsTextField" layoutX="66.0" layoutY="29.0" onAction="#rowsTextFieldOnAction" prefHeight="26.0" prefWidth="98.0" />
            <TextField fx:id="columnsTextField" layoutX="66.0" layoutY="63.0" onAction="#columnsTextFieldOnAction" prefHeight="26.0" prefWidth="98.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
